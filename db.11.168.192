;
; BIND reverse data file for local loopback interface
;
$TTL	3600
@	IN	SOA	z1.yanah.hier.  master.z1.yanah.hier. (
			      8		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
@	IN	NS	dns-z1.z1.yanah.hier.


; PTR Records
100	IN	PTR	dns-z1.z1.yanah.hier.	
101	IN	PTR	in.z1.yanah.hier.
102	IN	PTR	bacon-z1.yanah.hier.
103	IN	PTR	out.z1.yanah.hier.
