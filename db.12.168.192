;
; BIND reverse data file for local loopback interface
;
$TTL	3600
@	IN	SOA	z2.yanah.hier.  master.z1.yanah.hier. (
			      6		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800	 )	; Negative Cache TTL
;
; name servers
@	IN	NS	dsn-z2.z2.yanah.hier.

; PTR Records
100	IN	PTR	dns-z2.z2.yanah.hier.
101	IN	PTR	in.z2.yanah.hier.
102	IN	PTR	squirrelmail.z2.yanah.hier.
103	IN	PTR	out.z2.yanah.hier.
